{
  "name": "tarl-pratham-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=1024' next dev --turbopack",
    "dev:fresh": "rm -rf .next node_modules/.cache .turbopack && prisma generate && PORT=3003 NODE_OPTIONS='--max-old-space-size=1024' next dev --turbopack",
    "build": "NODE_OPTIONS='--max-old-space-size=1024' prisma generate && next build",
    "start": "NODE_OPTIONS='--max-old-space-size=1024' next start -p ${PORT:-3000}",
    "start:prod": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=1024' next start -p ${PORT:-3000}",
    "lint": "eslint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "update-passwords": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} scripts/update-all-passwords.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.21.3",
    "@ant-design/icons": "^6.0.2",
    "@ant-design/nextjs-registry": "^1.1.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@prisma/client": "^6.16.2",
    "@reactour/tour": "^3.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pdfkit": "^0.17.3",
    "@vercel/blob": "^2.0.0",
    "antd": "^5.27.4",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "dayjs": "^1.11.18",
    "idb": "^8.0.3",
    "next": "15.5.7",
    "next-auth": "^4.24.11",
    "pdfkit": "^0.17.2",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-confetti": "^6.4.0",
    "react-dom": "19.1.0",
    "react-is": "^18.2.0",
    "recharts": "^3.2.1",
    "sharp": "^0.34.4",
    "swiper": "^12.0.2",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "playwright": "^1.55.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
